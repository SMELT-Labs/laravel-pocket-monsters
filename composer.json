{
    "name": "smelt-labs/laravel-pocket-monsters",
    "description": "A Laravel wrapper for pokeapi",
    "autoload": {
        "psr-4": {
            "SmeltLabs\\PocketMonsters\\": "src/SmeltLabs/PocketMonsters"
        }
    },
    "require": {
        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SmeltLabs\\PocketMonsters\\PocketMonsterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
